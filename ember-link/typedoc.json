{
  "entryPoints": [
    "./src/index.ts",
    "./src/test-support/index.ts"
  ],
  "out": "../docs/api",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-vitepress-theme"
  ],
  "docsRoot": "../docs",
  "useCodeBlocks": true,
  "parametersFormat": "table",
  "typeDeclarationFormat": "table",
  "textContentMappings": {
    "breadcrumbs.home": "API",
    "kind.typeAlias.singular": "Type",
    "kind.typeAlias.plural": "Types",
  }
}