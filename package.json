{
  "name": "ember-link",
  "version": "1.4.0-beta.0",
  "description": "Link primitive to pass around self-contained route references",
  "keywords": [
    "ember-addon",
    "routing",
    "router",
    "template",
    "helper",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:buschtoens/ember-link.git"
  },
  "license": "MIT",
  "author": "Jan Buscht√∂ns <jan@buschtoens.me> (https://jan.buschtoens.me)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --ext js,ts .",
    "lint:types": "tsc --noEmit",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "@glimmer/tracking": "~1.0.3",
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-typescript": "^4.1.0",
    "fast-json-stable-stringify": "^2.1.0"
  },
  "devDependencies": {
    "@clark/ember-template-lint-config": "0.2.2",
    "@clark/eslint-config-ember": "1.28.2",
    "@clark/eslint-config-ember-typescript": "1.28.2",
    "@clark/eslint-config-node": "2.1.0",
    "@ember/optional-features": "2.0.0",
    "@glimmer/component": "1.0.4",
    "@types/ember": "3.16.5",
    "@types/ember-qunit": "3.4.15",
    "@types/ember-test-helpers": "1.0.9",
    "@types/ember-testing-helpers": "0.0.4",
    "@types/ember__test-helpers": "2.0.2",
    "@types/qunit": "2.11.1",
    "@types/rsvp": "4.0.3",
    "@types/sinon": "10.0.6",
    "broccoli-asset-rev": "3.0.0",
    "ember-array-helper": "5.1.0",
    "ember-cli": "3.24.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-typescript-blueprints": "3.0.0",
    "ember-cli-terser": "4.0.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-on-modifier": "1.0.1",
    "ember-qunit": "4.6.0",
    "ember-resolver": "8.0.3",
    "ember-sinon-qunit": "5.0.0",
    "ember-source": "3.25.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "3.13.0",
    "ember-try": "2.0.0",
    "eslint": "7.32.0",
    "loader.js": "4.7.0",
    "p-defer": "3.0.0",
    "qunit-dom": "1.6.0",
    "release-it": "14.11.7",
    "release-it-lerna-changelog": "3.1.0",
    "typescript": "3.9.10"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
