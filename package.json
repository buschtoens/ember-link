{
  "name": "ember-link",
  "version": "2.0.1",
  "description": "Link primitive to pass around self-contained route references",
  "keywords": [
    "ember-addon",
    "routing",
    "router",
    "template",
    "helper",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:buschtoens/ember-link.git"
  },
  "license": "MIT",
  "author": "Jan Buscht√∂ns <jan@buschtoens.me> (https://jan.buschtoens.me)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --ext js,ts .",
    "lint:types": "tsc --noEmit",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "resolutions": {
    "prettier": "2.8.1"
  },
  "dependencies": {
    "@glimmer/tracking": "^1.0.3",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-cli-typescript": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@clark/eslint-config-ember": "2.2.0",
    "@clark/eslint-config-ember-typescript": "2.2.0",
    "@clark/eslint-config-node": "2.2.0",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.6.0",
    "@glimmer/component": "1.1.2",
    "@glimmer/env": "0.1.7",
    "@types/ember": "3.16.7",
    "@types/ember-qunit": "5.0.2",
    "@types/ember-test-helpers": "1.0.11",
    "@types/ember-testing-helpers": "0.0.4",
    "@types/ember__debug": "3.16.5",
    "@types/ember__test-helpers": "2.6.1",
    "@types/qunit": "2.19.3",
    "@types/rsvp": "4.0.4",
    "@types/sinon": "10.0.13",
    "broccoli-asset-rev": "3.0.0",
    "ember-array-helper": "5.1.0",
    "ember-auto-import": "2.5.0",
    "ember-cli": "4.9.2",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-cli-typescript-blueprints": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-on-modifier": "1.0.1",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-sinon-qunit": "6.0.0",
    "ember-source": "3.25.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.7.3",
    "ember-try": "2.0.0",
    "eslint": "7.32.0",
    "loader.js": "4.7.0",
    "p-defer": "4.0.0",
    "qunit": "2.19.3",
    "qunit-dom": "2.0.0",
    "release-it": "15.5.1",
    "release-it-lerna-changelog": "5.0.0",
    "typescript": "4.4.3",
    "webpack": "5.75.0"
  },
  "engines": {
    "node": ">= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    }
  }
}
